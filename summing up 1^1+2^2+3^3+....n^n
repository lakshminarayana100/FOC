#include <stdio.h>
#include <math.h>

int main() {
    int n;
    long long sum = 0;
    
    printf("Enter the value of n: ");
    scanf("%d", &n);
    
    for (int i = 1; i <= n; i++) {
        long long term = pow(i, i);
        sum += term;
    }
    
    printf("The sum of the series is: %lld\n", sum);
    
    return 0;
}
